package common;

public class MvcUtils {

	/**
	 * 단방향 암호화 알고리즘
	 * - md5
	 * - sha1 : 160byte
	 * - sha256 : 256byte
	 * - sha512 : 512byte
	 * 
	 * byte가 높을수록 보안성이 좋다
	 * 
	 */
	public static String getSha512(String password) {
		String encryptedPassword = null;
		
		return encryptedPassword;
	}

}
